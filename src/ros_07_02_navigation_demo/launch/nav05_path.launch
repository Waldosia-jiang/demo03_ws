<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
		<param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/move_base_params.yaml" command="load" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/base_local_planner_params.yaml" command="load" />
		<rosparam file="$(find ros_07_02_navigation_demo)/param_1/dwa_local_planner_params.yaml" command="load" />
	</node>
</launch>
